<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asanaapp">
    android:versionCode="1"
    android:versionName="1.0">
    <uses-sdk android:minSdkVersion="11" android:targetSdkVersion="11"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoggedInActivity"/>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".RegisteredActivity"/>
        <activity android:name=".BeginnerAsanas"/>
        <activity android:name=".PracticeActivity"/>
        <activity android:name=".ShowDetail"/>
    </application>

</manifest>